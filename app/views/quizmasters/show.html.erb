<%- model_class = Quizmaster -%>
<div class="page-header">
  <h1><%=t '.title', :default => Quizmaster %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:quizid) %></dt>
      <dd><pre class="prettyprint"><%= @quizmaster.quizid %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:score) %></dt>
      <dd><pre class="prettyprint"><%= @quizmaster.score %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              quizmasters_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_quizmaster_path(@quizmaster), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              quizmaster_path(@quizmaster),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

